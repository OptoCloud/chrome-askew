{
    "manifest_version": 3,
   
    "name": "Askew",
    "version": "1.0.0.0",
    "description": "Askew is a browser extension that drives its users to the edge of insanity.",
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
   
    "content_scripts": [{
      "js": ["behaviour.js"],
      "css": ["styles.css"],
      "matches": [ "*://*/*" ]
    }]
  }